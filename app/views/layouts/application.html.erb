<!DOCTYPE html>
<html>
<head>
   <title>TestOmniDevise</title>
   <%= stylesheet_link_tag :all %>
   <%= javascript_include_tag :defaults %>
   <%= csrf_meta_tag %>
</head>
<body>                       
	
<section id="user">
    <% if user_signed_in? %>
        <nav id="nav_user">
            <ul>
                <span><%= current_user.email %></span>
                <li><%= link_to "Sign out", logout_path %></li>
            </ul>
        </nav>
    <% else %>
        <nav id="nav_user">
            <ul>
              <span>Sign in with Devise:</span>
              <li><%= link_to "Google Apps", user_omniauth_authorize_path(:google_apps) %></li>
<!--              <li><%# link_to "Git Hub", user_omniauth_authorize_path(:github) %></li> -->
            </ul>
        </nav>
    <% end %>
</section>

	<section id="flash">Flash:
		<% flash.each do |key, value| %>
			<div class="flash <%= key %> round"><%= value %></div>
		<% end %>
	</section>   

<%= yield %>


</body>
</html>
